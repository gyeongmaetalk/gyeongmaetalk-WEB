{
  "name": "web",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "icons": "pnpm exec svgr --out-dir app/components/icons -- app/assets/icons",
    "icons:watch": "npm-watch icons",
    "dev:svg": "run-s icons icons:watch",
    "dev:remix": "react-router dev",
    "dev": "run-p dev:*",
    "lint": "pnpm dlx eslint --fix",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "dependencies": {
    "@gyeongmaetalk/hooks": "workspace:*",
    "@gyeongmaetalk/ui": "workspace:*",
    "@gyeongmaetalk/utils": "workspace:*",
    "@hookform/resolvers": "^5.2.1",
    "@react-router/fs-routes": "^7.6.3",
    "@react-router/node": "^7.5.3",
    "@react-router/serve": "^7.5.3",
    "@tanstack/react-query": "^5.81.5",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tosspayments/tosspayments-sdk": "^2.4.0",
    "embla-carousel-autoplay": "^8.6.0",
    "isbot": "^5.1.27",
    "ky": "^1.8.1",
    "lucide-react": "^0.539.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.62.0",
    "react-router": "^7.5.3",
    "zod": "^4.1.3",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@gyeongmaetalk/eslint-config": "workspace:*",
    "@gyeongmaetalk/tailwind-config": "workspace:*",
    "@gyeongmaetalk/types": "workspace:*",
    "@gyeongmaetalk/typescript-config": "workspace:*",
    "@react-router/dev": "^7.5.3",
    "@svgr/cli": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@tailwindcss/vite": "^4.1.4",
    "@types/navermaps": "^3.9.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.30.1",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.3.0",
    "msw": "^2.11.1",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.13.0",
    "playwright": "^1.54.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.4",
    "tw-animate-css": "^1.3.7",
    "typescript": "^5.8.3",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.10.0+sha512.d615db246fe70f25dcfea6d8d73dee782ce23e2245e3c4f6f888249fb568149318637dca73c2c5c8ef2a4ca0d5657fb9567188bfab47f566d1ee6ce987815c39",
  "watch": {
    "icons": {
      "patterns": [
        "icons"
      ],
      "extensions": "svg",
      "quiet": false
    }
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
